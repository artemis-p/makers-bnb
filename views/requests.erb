<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Requests</title>
  </head>
  <body>
    <ul>
      <%@bookings_requested.each do |request| %>
      <li><%=request.space_name%>
        <%=request.space_description%>
        <%=request.space_price%>
        <%=request.booking_start_date%>
        <%=request.booking_end_date%>
        <%=request.booking_confirmation%></li>
    <%end%>
    </ul>
    <ul>
      <%@bookings_received.each do |received| %>
      <li><%=received.space_name%>
        <%=received.space_description%>
        <%=received.space_price%>
        <%=received.booking_start_date%>
        <%=received.booking_end_date%>
        <%=received.booking_confirmation%></li>
    <%end%>
    </ul>
  </body>
</html>
